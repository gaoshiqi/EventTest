<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kongge.eventtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".otto.OTTOMainActivity" />
        <activity android:name=".otto.OTTOSecondActivity" />
        <activity android:name=".eventbus.EventBusMainActivity" />
        <activity android:name=".eventbus.EventBusSecondActivity" />
    </application>

</manifest>